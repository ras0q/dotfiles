{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "github>aquaproj/aqua-renovate-config#2.6.0"
  ],
  "packageRules": [
    {
      "description": "Automerge non-major update",
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    },
    {
      "matchPackageNames": [
        "neovim/neovim"
      ],
      "enabled": false
    }
  ]
}
