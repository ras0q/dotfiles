{
  "nativeMCPServers": {
    "mcphub": {
      "autoApprove": [
        "get_current_servers",
        "toggle_mcp_server"
      ]
    },
    "neovim": {
      "autoApprove": [
        "execute_lua",
        "execute_command",
        "read_file",
        "move_item",
        "read_multiple_files",
        "delete_items",
        "find_files",
        "list_directory",
        "write_file",
        "edit_file"
      ]
    }
  },
  "servers": {
    "context7": {
      "autoApprove": [
        "resolve-library-id",
        "query-docs"
      ],
      "type": "streamable-http",
      "url": "https://mcp.context7.com/mcp"
    },
    "deepwiki": {
      "autoApprove": [
        "read_wiki_contents",
        "ask_question"
      ],
      "type": "stdio",
      "url": "https://mcp.deepwiki.com/mcp"
    },
    "gopls": {
      "args": [
        "mcp"
      ],
      "autoApprove": [
        "go_diagnostics",
        "go_file_context",
        "go_package_api",
        "go_rename_symbol",
        "go_search",
        "go_symbol_references",
        "go_vulncheck",
        "go_workspace"
      ],
      "command": "gopls",
      "cwd": "${PWD}"
    }
  }
}
